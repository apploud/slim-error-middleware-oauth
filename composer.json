		"phpstan/phpstan-strict-rules": "^0.12",
		"phpstan/phpstan-phpunit": "^0.12",
		"consistence/coding-standard": "^3.10",
		"slevomat/coding-standard": "^6.3",
		"phing/phing": "^2.16"
